<Window x:Class="CutiApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CutiApp"
        mc:Ignorable="d"
        Title="LEAVE REQUEST" Height="250" Width="450">
    <Grid>
        <GroupBox x:Name="groupLogin" Header="Login Page" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="191" Width="414">
            <StackPanel Orientation="Vertical" Margin="0,0,0,7">
                <Label Content="Email"></Label>
                <TextBox x:Name="emailTxt" Margin="0,0,243,0"></TextBox>
                <Label Content="Password"></Label>
                <TextBox x:Name="priceTxt" Margin="0,0,243,0"></TextBox>
            </StackPanel>
        </GroupBox>
    </Grid>
</Window>
